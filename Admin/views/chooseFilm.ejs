<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
<%- include('partials/navbar') %>
<body class="gradient-custom-2">
<div class="container mt-5">
  <h1 class="mt-5 mb-5 mx-auto">Choose a Film</h1>
<form action="/seeMore" method="post" class="mx-auto">
  <select id="movie" name="someName" class="form-control mt-3 mb-3">
    <% while (iterator> 0) {%>
      <option value="<%=result[iterator-1].Film_ID%>" name="<%=result[iterator-1].Title%>">
        <%=result[iterator-1].Title%></option>
      <% iterator -=1 } %>
    </select>
    <button id="seeMoreButton" class="btn btn-primary mt-3">See More</button>
  </form>
</div>
</body>