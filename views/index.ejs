      <div id="now_showing" class="gallery js-flickity" data-flickity-options='{ "wrapAround": true }'></div>

      <script>
        const numNovies = 7;
        for (var i = 0; i < numNovies; i++) {
          var movie = document.createElement("div");
          movie.classList.add("gallery-cell", "movie-card");
          movie.innerHTML += `
        <div class="moviethumb">
          <a href="">
            <img class="moviecover"
              src="../images/9cc20ec27e262e6b503db2b8e39e451b26e4ffe864a96292684a582de300ba4ec24c3f37915c2a545a16a3f3fd9ee9180ea892e5e33d5c330d3bc9_1280.jpg"
              alt="">
